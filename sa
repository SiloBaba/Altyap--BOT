const Discord = require('discord.js');

exports.run = (client, message, args) => {
      const snekfetch = require("snekfetch");
snekfetch.get(`https://discordbots.org/api/bots/${client.user.id}/check?userId=${message.author.id}`)
.set("Authorization", client.ayarlar.dbltoken)
.then(response => {
var check = response.body.voted;
if(check == 1) {
  //if(message.author.id !== "486817385051979786") return message.reply("Bu komut ÅŸuanda gÃ¼ncellenmektedir!")
  
  const db = require('quick.db');
  
 
 
        let member = message.guild.member(message.mentions.users.array()[0] || message.guild.members.get(args[0]))
        let member2 = message.guild.member(message.mentions.users.array()[1] || message.guild.members.get(args[1]))
        var s = message.author
        if(member2) {
                var s = member2.user
        }
        if(!member) {
                const embed = new Discord.RichEmbed()
                        .setDescription('LÃ¼tfen bir Ã¼yeyi etiketleyiniz')
                        .setColor("RANDOM")
                message.channel.send({embed})
                return
        }
 
        var anasonuc = Math.floor(Math.random() * 101)
        var kalp = ''
        var akalp = ''
        if(Math.floor(Math.round(anasonuc / 10) * 10) >= 10) {
                var c = 0
                for(var i = 0; i < Math.floor(Math.round(anasonuc / 10)); i++) {
                        kalp += 'â¤ï¸'
                        c++
                }
                for(var x = c; x < 10; x++) {
                        akalp += `ğŸ–¤`
                }
        } else {
                var kalp = 'ğŸ–¤'
                var akalp = 'ğŸ–¤ğŸ–¤ğŸ–¤ğŸ–¤ğŸ–¤ğŸ–¤ğŸ–¤ğŸ–¤ğŸ–¤'
        }
  var yorum = "Sizi evlendirelim <3"
        if(anasonuc < 80) {
                var yorum = "Biraz daha uÄŸraÅŸÄ±rsan bu iÅŸ olacak gibi :)"
        }
        if(anasonuc < 60) {
                var yorum = "Eh biraz biraz bir ÅŸeyler var gibi."
        }
        if(anasonuc < 40) {
                var yorum = "AzÄ±cÄ±kta olsa bir ÅŸeyler hissediyor sana :)"
        }
        if(anasonuc < 20) {
                var yorum =  "Bu iÅŸ olmaz sen bunu unut."
        }
  
        const embed = new Discord.RichEmbed()
                .setAuthor(`${member.user.tag} ve ${s.tag}`)
                .setDescription(`AÅŸk yÃ¼zdesi **%${anasonuc}**! \n${kalp}${akalp} \n\n${yorum}`)
                .setColor("RANDOM")
        message.channel.send({embed})
  
  
    
} else {
let embed = new Discord.RichEmbed()
      .setTitle('HATA')
      .setColor('RANDOM')
      .setDescription(`${client.emojis.get(client.emojiler.hayÄ±r)} **Hata**, bu komutu kullanmak iÃ§in **12 saat aralÄ±kla** **[BURADAN](https://discordbots.org/bot/${client.user.id}/vote)**  botu oylamanÄ±z gerekmektedir. OnaylanmasÄ± **1-4** dakikayÄ± bulabilir, lÃ¼tfen bekleyin. `)
    message.channel.send(embed)
      return }});
};

exports.conf = {
