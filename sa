        request(url, function (err, response, body) {
            if (err) {
                console.log(err);
                return message.channel.sendEmbed(new Discord.RichEmbed().setTitle('Hata!').addField('Sunucu bilgileri alÄ±nÄ±rken beklenmedik bir hatayla karÅŸÄ±laÅŸtÄ±k.').setThumbnail("https://cdn.pixabay.com/photo/2013/07/12/19/25/minecraft-154749_960_720.png").setAuthor("XERESSA").setFooter('').setColor("RED").setTimestamp());
            }
            body = JSON.parse(body);
            var status = '**âŒª** Sunucu Â» **' + reason + '**\n\n**âŒª** Sunucu ÅŸu anda aktif mi Â» **HayÄ±r**\n\n**âŒª** Bu IP adresi bir sunucuya ait deÄŸil veya sunucu ÅŸu anda kapalÄ±.';
            if (body.online) {
			status = '**âŒª** Sunucu adÄ± Â» **' + reason + '**\n\n**âŒª** Sunucu ÅŸu anda aktif mi Â» **Evet**\n\n**âŒª** Sunucu versiyonu Â» **'+ body.server.name +'**\n\n';
