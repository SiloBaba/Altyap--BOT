exports.run = (client, message, args) => {

  //if (!message.member.hasPermission("MANAGE_MESSAGES")) return message.reply(`Bu komutu kullanabilmek iÃ§in **MesajlarÄ± YÃ¶net** iznine sahip olmalÄ±sÄ±n!`);
  
  let user = message.mentions.users.first();
  if (message.mentions.users.size < 1) return message.reply('UyarÄ±larÄ±nÄ± kaldÄ±racaÄŸÄ±n kiÅŸiyi etiketlemelisin!');
  
  if (db.has(`uyarÄ±lar_${user.id}`) === false) return message.reply("Bu kullanÄ±cÄ±nÄ±n hiÃ§ uyarÄ±sÄ± bulunmuyor!")
  
  db.delete(`uyarÄ±lar_${user.id}`)
  
  const embed = new Discord.RichEmbed()
  .setColor("RANDOM")
  .setAuthor(`${user.username} - UyarÄ± Bilgisi`, user.avatarURL)
  .setDescription(`<@${user.id}> adlÄ± kullanÄ±cÄ±nÄ±n baÅŸarÄ±yla uyarÄ±larÄ± kaldÄ±rÄ±ldÄ±!`)
  .setFooter(`${client.user.username} - UyarÄ± Sistemi`, client.user.avatarURL)
  message.channel.send({embed})
  
};

