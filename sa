                const embed = new Discord.RichEmbed()
                        .setAuthor("Resim baÅŸarÄ±yla ayarlandÄ±!")
                        .setImage(args[1])
                        .setColor("RANDOM")
                msg.channel.send({embed})
                return
        }
  
