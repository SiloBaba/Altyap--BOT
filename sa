exports.run = async (client, message, args) => {
  
  const db = require('quick.db');
  
 if(!args[0]) return message.channel.send('LÃ¼tfen aranacak emojinin adÄ±nÄ± giriniz.')
    const emojii = client.emojis.find(e => e.name == args[0])

 if(emojii === null) return message.channel.send('BÃ¶yle bir emoji bulunamadÄ±. UnutmayÄ±n emojiler sadece botun **ekli** olduÄŸu sunucularda aramaktadÄ±r.')

  const emoji = client.emojis.find(e => e.name == args[0]).id

  
  message.channel.send(`${client.emojis.get(emoji)}`)
 
}

