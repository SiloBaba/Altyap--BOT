exports.run = async(client, message, args) => {
    let msg = await message.channel.send("Resim aranÄ±yor...");

    let {body} = await superagent 
    .get('https://aws.random.cat/meow');
    if(!{body}) return message.channel.send("Bir hata oluÅŸtu. Tekrar deneyiniz.")

    const embed = new Discord.RichEmbed()
    .setColor('GREEN')
    .setDescription("**"+message.author.username+"**, Ä°ÅŸte bir kedi !")
    .setImage(body.file)
    .setTimestamp()
    message.channel.send({embed})


    msg.delete();
};

