`, {split: true, code: "md"})

          
}
 } else {
let embed = new Discord.RichEmbed()
      .setTitle('HATA')
      .setColor('RANDOM')
      .setDescription(`${client.emojis.get(client.emojiler.hayÄ±r)} **Hata**, bu komutu kullanmak iÃ§in **12 saat aralÄ±kla** **[BURADAN](https://discordbots.org/bot/${client.user.id}/vote)**  botu oylamanÄ±z gerekmektedir. OnaylanmasÄ± **1-4** dakikayÄ± bulabilir, lÃ¼tfen bekleyin. `)
    msg.channel.send(embed)
      return }});
}

	exports.conf = {
		enabled: true,
		guildOnly: false,
		aliases: ['tag-ara'],
		permLevel: 0,
    kategori: "kullanÄ±cÄ±",
	};
	  
	exports.help = {
		name: 'discrim',
		description: 'Ä°stediÄŸiniz bir discrimi botun ekli olduÄŸu sunucularda arar',
		usage: 'discrim',
    
	};
const Discord = require('discord.js');
const { stripIndents } = require('common-tags');
const { randomRange, verify } = require('../util/Util.js');

exports.run = async (client, message, args) => {
  




  this.fighting = new Set();
  
        let opponent = message.mentions.users.first()
        if (!opponent) return message.reply("<:ak:617145990742278144> Oynamak istediÄŸin kiÅŸiyi etiketlemelisiniz.")
  
  if (opponent.bot) return message.reply('<:ak:617145990742278144> Botlar ile dÃ¼ello yapamazsÄ±n dostum.');
  if (opponent.id === message.author.id) return message.reply('<:ak:617145990742278144> Kendin ile dÃ¼ello atamazsÄ±n dostum.');
                if (this.fighting.has(message.channel.id)) return message.reply('Kanal sayÄ±sÄ±na gÃ¶re savaÅŸ yapabilirsin.');
                this.fighting.add(message.channel.id);
                try {
                        if (!opponent.bot) {
                await message.channel.send(`${opponent}, dÃ¼ello isteÄŸi geldi. DÃ¼ello'yu kabul ediyor musun? (\`evet\` veya \`hayÄ±r\` olarak cevap veriniz.)`);
                                const verification = await verify(message.channel, opponent);
                                if (!verification) {
                                        this.fighting.delete(message.channel.id);
                                        return message.channel.send(`<:ak:617145990742278144> DÃ¼ello kabul edilmedi...`);
                                }
                        }
                        let userHP = 500;
                        let oppoHP = 500;
                        let userTurn = false;
                        let guard = false;
                        const reset = (changeGuard = true) => {
                                userTurn = !userTurn;
                                if (changeGuard && guard) guard = false;
                        };
                        const dealDamage = damage => {
                                if (userTurn) oppoHP -= damage;
                                else userHP -= damage;
                        };
                        const forfeit = () => {
                                if (userTurn) userHP = 0;
                                else oppoHP = 0;
                        };
                        while (userHP > 0 && oppoHP > 0) { // eslint-disable-line no-unmodified-loop-condition
                                const user = userTurn ? message.author : opponent;
                                let choice;
                                if (!opponent.bot || (opponent.bot && userTurn)) {
                                        await message.channel.send(stripIndents`
                                                ${user}, ne yapmak istersin? \`saldÄ±r\`, \`savun\`, \`ultra gÃ¼Ã§\`, veya \`kaÃ§\`?
                                                **${message.author.username}**: ${userHP} :heartpulse:
                                                **${opponent.username}**: ${oppoHP} :heartpulse:
                                        `);
                                        const filter = res =>
                                                res.author.id === user.id && ['saldÄ±r', 'savun', 'ultra gÃ¼Ã§', 'kaÃ§'].includes(res.content.toLowerCase());
                                        const turn = await message.channel.awaitMessages(filter, {
                                                max: 1,
                                                time: 30000
                                        });
                                        if (!turn.size) {
                                                await message.reply(`ÃœzgÃ¼nÃ¼m ama, sÃ¼re doldu!`);
                                                reset();
                                                continue;
                                        }
                                        choice = turn.first().content.toLowerCase();
                                } else {
                                        const choices = ['saldÄ±r', 'savun', 'ultra gÃ¼Ã§'];
                                        choice = choices[Math.floor(Math.random() * choices.length)];
                                }
                                if (choice === 'saldÄ±r') {
                                        const damage = Math.floor(Math.random() * (guard ? 10 : 100)) + 1;
                                        await message.channel.send(`${user}, **${damage}** hasar vurdu!`);
                                        dealDamage(damage);
                                        reset();
                                } else if (choice === 'savun') {
                                        await message.channel.send(`${user}, kendisini sÃ¼per kalkan ile savundu!`);
                                        guard = true;
                                        reset(false);
                                } else if (choice === 'ultra gÃ¼Ã§') {
                                        const miss = Math.floor(Math.random() * 4);
                                        if (!miss) {
                                                const damage = randomRange(100, guard ? 150 : 300);
                                                await message.channel.send(`${user}, Ã‡oook uzak galaksilerden gelen ultra sonik enerjiyi yeterki miktarda topladÄ±n ve **${damage}** hasar vurdun!!`);
                                                dealDamage(damage);
                                        } else {
                                                await message.channel.send(`${user}, Ã‡oook uzak galaksilerden gelen ultra sonik enerjiyi yeterli miktarda toplayamadÄ±ÄŸÄ±n iÃ§in ulta gÃ¼Ã§ kullanamadÄ±n!`);
                                        }
                                        reset();
                                } else if (choice === 'kaÃ§') {
                                        await message.channel.send(`${user}, kaÃ§tÄ±! Korkak!`);
                                        forfeit();
                                        break;
                                } else {
                                        await message.reply('Ne yapmak istediÄŸini anlamadÄ±m.');
                                }
                        }
                        this.fighting.delete(message.channel.id);
            const winner = userHP > oppoHP ? message.author : opponent;
                        return message.channel.send(`Oyun bitti! Tebrikler, **${winner}** kazandÄ±! \n**${message.author.username}**: ${userHP} :heartpulse: \n**${opponent.username}**: ${oppoHP} :heartpulse:`);
                } catch (err) {
                        this.fighting.delete(message.channel.id);
                        throw err;
                }
  }

