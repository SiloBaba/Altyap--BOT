    message.reply('kullanÄ±cÄ± deÄŸil, bir oyuncu adÄ± belirtmelisin :/')
 } else {
 const mcbody = new Discord.RichEmbed()
   .setColor('RED')
   .setTitle('Oyuncu: ' + mesaj)
   .setImage(body)
 message.channel.send(mcbody);
}
       }
  	if (args[0] === 'kafa') {
 let member = message.mentions.members.first();
 let body = 'https://mc-heads.net/head/' + mesaj
 if (mesaj.length < 1) return message.reply('bir oyuncu adÄ± belirtmelisin.');
 if (mesaj == member) {
    message.reply('kullanÄ±cÄ± deÄŸil, bir oyuncu adÄ± belirtmelisin :/')
