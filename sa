exports.run = async (client, message, args) => {
  
  let user = client.users.get(args.slice(0).join(' '));
  if (!user) {
    let e = new Discord.RichEmbed()
    .setColor("RANDOM")
    .setDescription("Kara listeden kaldÄ±rmak istediÄŸin kullanÄ±cÄ±nÄ±n ID'ini yaz!")
    message.channel.send({embed: e})
    return;
  };
  
  //if (db.has(`karalist_${user.id}`) === false) return message.reply("Bu kullanÄ±cÄ± kara listede deÄŸil!")
  
  db.delete(`karalist_${user.id}`)
  
  let embed = new Discord.RichEmbed()
    .setColor("RANDOM")
    .setDescription(`${user.tag} adlÄ± kullanÄ±cÄ± baÅŸarÄ±yla kara listeden Ã§Ä±kartÄ±ldÄ±!`)
    message.channel.send({embed: embed})
  
};

