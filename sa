exports.run = async (client, message, args) => {
   
  const db = require('quick.db');
  let prefix = await db.fetch(`prefix_${message.guild.id}`) || client.ayarlar.prefix;
  let channel = message.mentions.channels.first() || message.guild.channels.find(c=>c.name===args.slice(0).join(' '))
  
   if(args[0] === 'kapat') {
   if (db.has(`otoRK_${message.guild.id}`) === true) {
     message.channel.send(`Otorol kayÄ±t kanalÄ± baÅŸarÄ±yla kaldÄ±rÄ±ldÄ±`)
     db.delete(`otoRK_${message.guild.id}`)
     return
}
  message.channel.send(`Otorol kayÄ±t kanalÄ± ayarlanmamÄ±ÅŸ.`)
    return
   }
  
  if (db.has(`otoR_${message.guild.id}`) === false) return message.channel.send(`Bu ayarÄ± kullanmanÄ±z iÃ§in Ã¶nce otorol ayarlamanÄ±z gerekmektedir.`)
  
    if (!channel) {
        return message.reply("Otomatik rol kayÄ±tlarÄ± kanalÄ± olarak ayarlamak istediÄŸiniz kanalÄ± etiketleyiniz!")
    }
  
     db.set(`otoRK_${message.guild.id}`, channel.id)
  
    const embed = new Discord.RichEmbed()
    .setDescription(`Otomatik rol kayÄ±tlarÄ± kanalÄ± baÅŸarÄ±yla ${channel} olarak ayarlandÄ±!\nOtorol kayÄ±t kanalÄ±nÄ± kapatmak isterseniz **${prefix}oto-rol-kanal kapat** yazmanÄ±z yeterlidir.`)
    .setColor("RANDOM")
    message.channel.send({embed})
  
};

