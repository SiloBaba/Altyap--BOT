exports.run = async (client, message, args) => {
  
  const db = require('quick.db');
  
 
  let kanal = message.mentions.channels.first(); 
  
  let prefix = await db.fetch(`prefix_${message.guild.id}`) || client.ayarlar.prefix;
  
    if(args[0] === 'kapat') {
   if (db.has(`destekK_${message.guild.id}`) === true) {
     message.channel.send(`Destek kanalÄ± baÅŸarÄ±yla kaldÄ±rÄ±ldÄ±`)
     db.delete(`destekK_${message.guild.id}`)
     return
}
  message.channel.send(`Destek kanalÄ± ayarlanmamÄ±ÅŸ.`)
    return
  
  }
    
  
    if (!kanal) {
      let e = new Discord.RichEmbed()
      .setDescription('LÃ¼tfen bir kanal etiketleyiniz')
      .setColor("RANDOM")
      message.channel.send(e)
      return;
    }
  
    var s = db.set(`destekK_${message.guild.id}`, kanal.id)
  
    const embed = new Discord.RichEmbed()
    .setDescription(`${client.emojis.get(client.emojiler.evet)} Destek kanalÄ± ayarlandÄ±: ${kanal}\nDestek kanalÄ±nÄ± kapatmak iÃ§in **${prefix}destek-kanal kapat** yazmanÄ±z yeterlidir.`)
    .setColor("RANDOM")
    message.channel.send({embed})
}

