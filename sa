.then(response => {
var check = response.body.voted;
if(check == 1) { 
  if(args[0].length > 4) return msg.channel.send('Discrimler 4 basamaklÄ± olur')
  const s = [`asd1`, `asd2`, `asd3`]
  	const discrim = args[0] || msg.author.discriminator;
        const users = client.users.filter(user => user.discriminator === discrim).map(user => user.tag);
        if (users < 1) {
            let embed = {
                color: 'RANDOM',
                description: `${discrim} bulunamadÄ±!`,
              };
            return msg.channel.send({embed});
          
        } else {
       
           msg.channel.send(`

