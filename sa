        .addField('YayÄ±nlanma tarihi', data.release_date.date || 'BulunamadÄ±')
				.addField('Dlc sayÄ±sÄ±', data.dlc ? data.dlc.length : "0")
				.addField('GeliÅŸtiriciler', data.developers ? data.developers.join(', ') || 'BulunamadÄ±' : 'BulunamadÄ±')
				.addField('YayÄ±ncÄ±lar', data.publishers ? data.publishers.join(', ') || 'BulunamadÄ±' : 'BulunamadÄ±');
