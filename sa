exports.run = (client, message, args) => {
    var command = args[0];
  if (!command) return message.reply("Bir komut adÄ± yazmalÄ±sÄ±n!")
    message.channel.send("`" + command + "` adlÄ± komut yÃ¼kleniyor...")
      .then(m => {
        client.load(command)
          .then(() => {
            m.edit("`" + command + "` adlÄ± komut baÅŸarÄ±yla yÃ¼klendi.");
          })
          .catch(e => {
            m.edit("`komutlar` klasÃ¶rÃ¼nde `"+command+".js` isminde bir dosya yok!");
          });
      });
};

