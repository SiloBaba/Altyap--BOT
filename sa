      let duration = args[0];
      let sure = args[1];
      let sebep = args[2];
      let user = message.author
      let bisi;
	  
	  if (!duration || duration >= '60')
	  {
      return message.reply(`LÃ¼tfen, geÃ§erli bir sÃ¼re belirtiniz.\nÃ–rnek olarak:!!hatÄ±rlat 10 dakika sebep`);
      }

