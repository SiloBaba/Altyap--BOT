const Discord = require('discord.js')
const fs = require('fs');


exports.run = async (client, message, args) => {

  const db = require('quick.db');
  

  let role = message.mentions.roles.first() || message.guild.roles.find(r => r.name === args.slice(0).join(' '));
  let prefix = await db.fetch(`prefix_${message.guild.id}`) || client.ayarlar.prefix;
  
  
  

    if(args[0] === 'kapat') {
   if (db.has(`kayÄ±tR_${message.guild.id}`) === true) {
     message.channel.send(`KayÄ±t rolÃ¼ baÅŸarÄ±yla kaldÄ±rÄ±ldÄ±`)
     db.delete(`kayÄ±tR_${message.guild.id}`)
     return
}
  message.channel.send(`kayÄ±t rolÃ¼ ayarlanmamÄ±ÅŸ.`)
    return
  
  }

  
  
    if (!role) {
        return message.reply(`LÃ¼tfen bir rol etiketleyin veya rol adÄ±nÄ± yazÄ±n Ã¶rnek: **${prefix}kayÄ±trol @rol** veya **${prefix}kayÄ±trol rol-adÄ±** `)
    }

  
     db.set(`kayÄ±tR_${message.guild.id}`, role.id)
  
    const embed = new Discord.RichEmbed()
    .setDescription(`${client.emojis.get(client.emojiler.evet)} KayÄ±t rolÃ¼ baÅŸarÄ±yla ayarlandÄ±: **${role.name}**\nKayÄ±t rolÃ¼nÃ¼ kapatmak isterseniz **${prefix}kayÄ±trol kapat** yazmanÄ±z yeterlidir.`)
    .setColor("RANDOM")
    message.channel.send({embed})
  
};

