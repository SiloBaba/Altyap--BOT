let sk = message.mentions.channels.first();
let kanal = await db.fetch(`sk_${message.guild.id}`)
  
  if (args[0] === "sÄ±fÄ±rla" || args[0] === "kapat") {
    if(!kanal) return message.channel.send(`Seviye kanal sistemini kapatmak iÃ§in \`seviye kanalÄ±nÄ±n\` seÃ§ili olmasÄ± lazÄ±m Ã¶rnek kullanÄ±m: \`!!seviye-kanal #kanal\``);
    
   db.delete(`sk_${message.guild.id}`)
  
   message.channel.send(`Seviye kanalÄ± baÅŸarÄ±yla kapatÄ±ldÄ±.`);
 
  
    return
  }
  
if (!sk) return message.channel.send('Seviye kanalÄ±nÄ± bulamadÄ±m. KullanÄ±m !!seviye-kanal #kanal');
 

   db.set(`sk_${message.guild.id}`, sk.id)

 message.channel.send(`Seviye kanalÄ± ${sk} olarak ayarlandÄ±\nSÄ±fÄ±rlamak iÃ§in !!seviye-kanal sÄ±fÄ±rla`);





}


  
