exports.run = (client, message, args) => {

  const db = require('quick.db');
  

    
  if (!message.guild.members.get(client.user.id).hasPermission("BAN_MEMBERS")) return message.reply('Gerekli izin yok')
  //if (!message.member.hasPermission("BAN_MEMBERS")) return message.reply(`Bu komutu kullanabilmek iÃ§in **Ãœyeleri Yasakla** iznine sahip olmalÄ±sÄ±n!`);
  
  let user = message.mentions.users.first();
  let reason = args.slice(1).join(' ');
  //let modLog = JSON.parse(fs.readFileSync("./jsonlar/mLog.json", "utf8"));
  if (db.has(`log_${message.guild.id}`) === false) return message.reply('Mod log kanalÄ± ayarlanmamÄ±ÅŸ');
  let modlog = message.guild.channels.get(db.fetch(`log_${message.guild.id}`).replace("<#", "").replace(">", ""));
  if (message.mentions.users.size < 1) return message.reply('LÃ¼tfen banlamak istediÄŸiniz Ã¼yeyi etiketleyin');
  if (reason.length < 1) return message.reply('LÃ¼tfen sebep giriniz');
  if (user.id === message.author.id) return message.reply('Kendinimi banlayacaksÄ±n?');
  /*if (user.highestRole.calculatedPosition > message.member.highestRole.calculatedPosition - 1) {
			return message.reply(`Bu kiÅŸinin senin rollerinden/rolÃ¼nden daha yÃ¼ksek rolleri/rolÃ¼ var.`);
		}*/
  //if (!message.guild.member(user).bannable) return message.channel.send(`Bu kiÅŸiyi sunucudan yasaklayamÄ±yorum Ã§Ã¼nkÃ¼ \`benden daha yÃ¼ksek bir role sahip\` ya da \`bana gerekli yetkileri vermedin\`.`);
  
  const embed = new Discord.RichEmbed()
  .setColor("RANDOM")
  .addField('Ä°ÅŸlem', 'Ban')
  .addField('Banlanan Ã¼ye', `${user.tag} (${user.id})`)
  .addField('Banlayan yetkili', `${message.author.username}#${message.author.discriminator}`)
  .addField('Ban sebebi', "```" + reason + "```")
  modlog.send(embed);
  user.send(`\`${message.guild.name}\` AdlÄ± Sunucuda yaptÄ±ÄŸÄ±nÄ±z olumsuz davranÄ±ÅŸlardan dolayÄ± yasaklandÄ±nÄ±z\nYetkilinin girdiÄŸi sebep: \`${reason}\``)
  
   //if (!message.guild.member(user).bannable) return message.reply('Yetkilileri yasaklayamam!');
  message.guild.ban(user, 2);
  
  const embed2 = new Discord.RichEmbed()
  .setColor("RANDOM")
  .setDescription(`BaÅŸarÄ±yla banlandÄ±`)
  message.channel.send(embed2)

  
};

