exports.run = async (client, message, args) => {

  let x = /(-)/

  let user = message.mentions.users.first();

  if (!user) return message.reply('Kime altÄ±n yollayacaÄŸÄ±nÄ± etiketlemen lazÄ±m!');
  if (user.bot === true) return message.reply('altÄ±nÄ± bir bota yollarsan sen zararlÄ± Ã§Ä±karsÄ±n!');
  
  let mesaj = args.slice(1).join(' ');
  if (!mesaj) return message.reply('YollayacaÄŸÄ±n miktarÄ± yazman gerek!');

  if (user.id === message.author.id) return message.reply(`${client.emojis.get(client.emojiler.kendineParaYollama)} Kendine altÄ±nmÄ± yollayacaksÄ±n ciddimisin?`);

  if (mesaj.match(x)) return message.reply('Tek zeki sensin zaten!');
if (isNaN(args[1])) return message.channel.send("LÃ¼tfen bir sayÄ± gir.")

    
    

  let altÄ±n = await db.fetch(`altÄ±ncÄ±klar_${message.author.id}`);
  let elmas = await db.fetch(`altÄ±ncÄ±klar_${message.author.id}`);
  let para = await db.fetch(`paracÄ±k_${message.author.id}`);


  let prefix = await db.fetch(`prefix_${message.guild.id}`) || client.ayarlar.prefix;

 

  if (altÄ±n < mesaj) {
    let bulunanP = await db.fetch(`altÄ±ncÄ±klar_${message.author.id}`)
    message.channel.send(`${client.emojis.get(client.emojiler.paraGitmedi)} Yeterince altÄ±nÄ±n bulunmuyor, sende olan altÄ±n:  ${bulunanP === null  ? "0" : `${bulunanP}`}`)
} else if  (altÄ±n > mesaj) {

  const embed = new Discord.RichEmbed()
  .setColor('RANDOM')
  .setDescription(`${client.emojis.get(client.emojiler.paraGitti)} ${user} AdlÄ± kullanÄ±cÄ±ya altÄ±n yollandÄ±, yollanÄ±lan miktar: ${mesaj}`)
  message.channel.send(embed)
  db.add(`altÄ±ncÄ±klar_${user.id}`, mesaj)
  db.add(`altÄ±ncÄ±klar_${message.author.id}`, -mesaj)
  

}
}

