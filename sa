const Discord = require('discord.js')
const fs = require('fs');


exports.run = async (client, message, args) => {

  const db = require('quick.db');
  

  let role = message.mentions.roles.first() || message.guild.roles.find(r => r.name === args.slice(0).join(' '));
  let prefix = await db.fetch(`prefix_${message.guild.id}`) || client.ayarlar.prefix;
  
  
  

    if(args[0] === 'kapat') {
   if (db.has(`otoR_${message.guild.id}`) === true) {
     message.channel.send(`Otorol baÅŸarÄ±yla kaldÄ±rÄ±ldÄ±`)
     db.delete(`otoR_${message.guild.id}`)
     return
}
  message.channel.send(`Otorol ayarlanmamÄ±ÅŸ.`)
    return
  
  }

  
  
    if (!role) {
        return message.reply(`LÃ¼tfen bir rol etiketleyin Ã¶rnek: **${prefix}oto-rol-ayarla @rol**`)
    }

  
     db.set(`otoR_${message.guild.id}`, role.id)
  
    const embed = new Discord.RichEmbed()
    .setDescription(`Otorol baÅŸarÄ±yla ayarlandÄ±: **${role.name}**\nOtorRol kapatmak isterseniz **${prefix}otorol kapat** yazmanÄ±z yeterlidir.\nÅimdi ise !!oto-rol-kanal #kanal`)
    .setColor("RANDOM")
    message.channel.send({embed})
  
};

