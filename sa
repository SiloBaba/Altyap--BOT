                                const embed = new Discord.RichEmbed()
                                        .setDescription("Renk deÄŸiÅŸtirilmemiÅŸ neyi sÄ±fÄ±rlayacaksÄ±n!")
                                        .setColor("RANDOM")
                                msg.channel.send({embed})
                                return
                        }
                        db.delete(`${msg.author.id}.renk`)
                        const embed = new Discord.RichEmbed()
                                .setDescription("Renk baÅŸarÄ±yla sÄ±fÄ±rlandÄ±!")
                                .setColor("RANDOM")
                        msg.channel.send({embed})
                        return
                }
                if(!args[1]) {
