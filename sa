// Start with the character '!'
const OFFSET = '!'.charCodeAt(0);

exports.run = (bot, msg, args) => {
    if (args.length < 1) {
        msg.reply('<:ak:617145990742278144> Ters yapacaÄŸÄ±m kelimeyi/yazÄ±yÄ± yazmalÄ±sÄ±n.');
    }

    msg.channel.send(
        args.join(' ').split('')
            .map(c => c.charCodeAt(0) - OFFSET)
            .map(c => mapping[c] || ' ')
            .reverse().join('')
    );
  msg.react('617413726768988160')
};

