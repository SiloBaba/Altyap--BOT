exports.run = async (client, message, args) => {


  let x = /(-)/

 


    let altÄ±n = await db.fetch(`altÄ±ncÄ±klar_${message.author.id}`);
  let miktar = args.slice(0).join(' ');
  if (!miktar) return message.reply('AlacaÄŸÄ±n miktarÄ± yazman gerek!');


  if (miktar.match(x)) return message.reply('Tek zeki sensin zaten!');
if (isNaN(args[0])) return message.channel.send("LÃ¼tfen bir sayÄ± gir.")


  
  let para = await db.fetch(`paracÄ±k_${message.author.id}`);
  
  let eksilcek = -250
//  let sigara = 1
  let altinMiktar = 1
  let altinpara = 250
 
    if (para < altinpara*miktar) {
      message.channel.send(`:no_entry: ${miktar} AltÄ±n alabilmek iÃ§in yeterli paran gÃ¶rÃ¼nmÃ¼yor. \n Gerekli para **${miktar*altinpara} **`)
  } else if (para > miktar*altinpara) {
    
 const i = await db.add(`paracÄ±k_${message.member.id}`, -miktar*altinpara)
     db.add(`altÄ±ncÄ±klar_${message.member.id}`, miktar)
     const aldÄ±E2 = new Discord.RichEmbed()
     .setColor('RANDOM')
     .setDescription(`${client.emojis.get(client.emojiler.evet)} BaÅŸarÄ±yla marketten **${miktar}** altÄ±n aldÄ±nÄ±z\n HesabÄ±nÄ±zdan eksilen para miktarÄ±: **${miktar*altinpara}TL**`)
     .setTimestamp()
     message.channel.send(aldÄ±E2)
 
  } else if (para = miktar*altinpara) {
    
    const i = await db.add(`paracÄ±k_${message.member.id}`, -miktar*altinpara)
   db.add(`altÄ±ncÄ±klar_${message.member.id}`, miktar)
   const aldÄ±E = new Discord.RichEmbed()
     .setColor('RANDOM')
     .setDescription(`${client.emojis.get(client.emojiler.evet)} BaÅŸarÄ±yla marketten **${miktar}** altÄ±n aldÄ±nÄ±z\n HesabÄ±nÄ±zdan eksilen para miktarÄ±: **${miktar*altinpara}TL**`)
     .setTimestamp()
     message.channel.send(aldÄ±E)
}
