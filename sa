const Discord = require('discord.js');
//var ayarlar = require('../ayarlar.json');
const db = require('quick.db');

exports.run = async (client, message, args) => {
  
  const ayarlar = client.ayarlar
  
  var s = 'tr'
  var a = client.commands.get('dil-deÄŸiÅŸtir').help.name
    if(db.has(`dil_${message.guild.id}`) === true) {
        var s = 'en'
        var a = client.commands.get('dil-deÄŸiÅŸtir').help.enname
    }
    const dil = s
    const o = a
  
  var d = args.slice(0).join(' ');
  if (!d) {
    let e = new Discord.RichEmbed()
    .setColor(client.ayarlar.renk)
    .setDescription(client[dil].argerror.replace("{prefix}", ayarlar.prefix).replace("{komut}", o))
    message.channel.send(e)
    return;
  }
  
  if (d !== "en" && d !== "tr" && d !== "sÄ±fÄ±rla" && d !== "reset") {
    let e = new Discord.RichEmbed()
    .setColor(client.ayarlar.renk)
    .setDescription(client[dil].argerror.replace("{prefix}", ayarlar.prefix).replace("{komut}", o))
    message.channel.send(e)
    return;
  }
  
  if (d === "en") {
    
    var w = db.set(`dil_${message.guild.id}`, "en")
    
    if (dil === "tr") w = "Ä°ngilizce"
    if (dil === "en") w = "English"
    
    let e = new Discord.RichEmbed()
    .setColor("RANDOM")
    .setDescription(client[dil].dil.setd.replace("{dil}", w))
    message.channel.send(e)
    return;
    
  }
  
  if (d === "tr") {
    db.delete(`dil_${message.guild.id}`)
    
    let b = "tr"
    
    if (dil === "tr") b = "TÃ¼rkÃ§e"
    if (dil === "en") b = "Turkish"
    
    let e = new Discord.RichEmbed()
    .setColor("RANDOM")
    .setDescription(client[dil].dil.setd.replace("{dil}", b))
    message.channel.send(e)
    return;
    
  }
  
  if (d === "sÄ±fÄ±rla" || d === "reset") {
    
    db.delete(`dil_${message.guild.id}`)
    
    let e = new Discord.RichEmbed()
    .setColor("RANDOM")
    .setDescription(client[dil].dil.resetd)
    message.channel.send(e)
    return;
    
  }
  
};

