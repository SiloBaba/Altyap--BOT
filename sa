const Discord = require('discord.js');
const ayarlar = require('../ayarlar.json');
const db = require('quick.db');
//Yashinu (Akame Owner)
exports.run = async(client, message, args) => {
  let prefix = await require('quick.db').fetch(`prefix_${message.guild.id}`) || ayarlar.prefix
  if(!message.member.hasPermission('ADMINISTRATOR')) return message.reply('Bu komutu kullanabilmek iÃ§in `YÃ¶netici` iznine sahip olmalÄ±sÄ±n!')
  
  let panel = await db.fetch(`sunucupanel_${message.guild.id}`)
  if(args[0] === "sil" || args[0] === "kapat") {
    db.delete(`sunucupanel_${message.guild.id}`)
    try {
      await message.guild.channels.find(x => (x.name).includes("â€¢ Sunucu Panel")).delete()
      await message.guild.channels.find(x => (x.name).includes("Toplam Ãœye â€¢")).delete()
      await message.guild.channels.find(x => (x.name).includes("Aktif Ãœye â€¢")).delete()
      await message.guild.channels.find(x => (x.name).includes("Botlar â€¢")).delete()
      await message.guild.channels.find(x => (x.name).includes("Rekor Aktiflik â€¢")).delete()
    } catch(e) { }
    message.channel.send(`Ayarlanan sunucu paneli baÅŸarÄ±yla devre dÄ±ÅŸÄ± bÄ±rakÄ±ldÄ±!`)
   return 
  }

  if(panel) return message.channel.send(`Bu sunucuda panel zaten ayarlanmÄ±ÅŸ! DevredÄ±ÅŸÄ± bÄ±rakmak iÃ§in;  \`${prefix}sunucupanel sil\``)
  
  message.channel.send(new Discord.RichEmbed().setColor('RANDOM').setTitle('Sunucu Panel').setDescription('Gerekli dosaylar kurulsun mu?').setFooter('OnaylÄ±yorsanÄ±z 15 saniye iÃ§erisinde "evet" yazmalÄ±sÄ±nÄ±z.'))
  .then(() => {
    message.channel.awaitMessages(response => response.content === 'evet', {
      max: 1,
      time: 15000,
      errors: ['time'],
    }) 
    .then((collected) => { 
      try {
        let role = message.guild.roles.find("name", "@everyone");
        message.guild.createChannel(`${client.user.username} â€¢ Sunucu Panel`, 'category', [{id: message.guild.id, deny: ['CONNECT']}]);
        message.guild.createChannel(`Toplam Ãœye â€¢ ${message.guild.memberCount}`, 'voice').then(channel => channel.setParent(message.guild.channels.find(channel => channel.name === `${client.user.username} â€¢ Sunucu Panel`))).then(c => {
          c.overwritePermissions(role, {
            CONNECT: false,
          });
        })
        message.guild.createChannel(`Aktif Ãœye â€¢ ${message.guild.members.filter(off => off.presence.status !== 'offline').size}`, 'voice').then(channel => channel.setParent(message.guild.channels.find(channel => channel.name === `${client.user.username} â€¢ Sunucu Panel`))).then(c => {
          c.overwritePermissions(role, {
            CONNECT: false,
          });
        })
        message.guild.createChannel(`Botlar â€¢ ${message.guild.members.filter(m => m.user.bot).size}`, 'voice').then(channel => channel.setParent(message.guild.channels.find(channel => channel.name === `${client.user.username} â€¢ Sunucu Panel`))).then(c => {
          c.overwritePermissions(role, {
            CONNECT: false,
          });
        })
        message.guild.createChannel(`Rekor Aktiflik â€¢ ${message.guild.members.filter(off => off.presence.status !== 'offline').size}`, 'voice').then(channel => channel.setParent(message.guild.channels.find(channel => channel.name === `${client.user.username} â€¢ Sunucu Panel`))).then(c => {
          c.overwritePermissions(role, {
            CONNECT: false,
          });
        })
     
        db.set(`sunucupanel_${message.guild.id}`, message.guild.members.filter(off => off.presence.status !== 'offline').size)
        message.channel.send(`Sunucu panel iÃ§in gerekli kanallar oluÅŸturulup, ayarlamalar yapÄ±ldÄ±!  \`(Oda isimlerini deÄŸiÅŸtirmeyin, Ã§alÄ±ÅŸmaz!)\``)
      } catch(e) {
        console.log(e.stack);
      }
    });
  });
};

