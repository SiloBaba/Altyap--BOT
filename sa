const Discord = require("discord.js");
const bot = new Discord.Client();
const db = require('quick.db');
exports.run = (client, message, args) => {
  try{
  if (!message.member.hasPermission("ADMINISTRATOR")) return message.channel.send('Bu komutu kullanabilmek iÃ§in `YÃ¶netici` yetkisine sahip olmalÄ±sÄ±n!')

  const s2 = args[0]
  
  if(s2 === 'aÃ§' || s2 === 'ac')
    {
      db.set(`offline_${message.guild.id}`, 1)
      message.reply("Offline Etiket **aÃ§Ä±ldÄ±.**")
      
    }
  else if(s2 === 'kapa' || s2 === 'kapat')
    
    {
      db.set(`offline_${message.guild.id}`, 0)
       message.reply("Offline Etiket **kapatÄ±ldÄ±.**")
    }
    
    else
      {
        message.reply("!!offline-etiket aÃ§ | !!offline-etiket kapat olarak kullanÄ±n")
        
      }


     }catch(err) { console.log(err + "hata bulundu")}
}
