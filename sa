const Discord = require('discord.js');
const math = require('math-expression-evaluator')
const stripIndents = require('common-tags').stripIndents



exports.run = function(client, message, args) {
    var soru = args.join(' ');

    if(!soru) return message.reply(''+client.emojis.get(client.emojiler.kapalii)+' LÃ¼tfen bir iÅŸlem yazÄ±n. Ã–rnek: ``!!matematik 16*40``')
    else { let cevap;
        try {
            cevap = math.eval(soru)
        } catch(err) {
           if (message.channel.send(''+client.emojis.get(client.emojiler.kapalii)+' HatalÄ± iÅŸlem: ' + 'LÃ¼tfen Ã§arpma iÅŸlemi yaparken \`x\` yerine \` * \` kullanÄ±n.')){
             
           } else return
        } 

        message.reply(`\nÄ°ÅŸlem : \`${soru}\`\nCevap: \`${cevap}\``)
          
         }


};  

