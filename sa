const Discord = require('discord.js')
const numerals = {
                                        "M": 1000,
                                        "CM": 900,
                                        "D": 500,
                                        "CD": 400,
                                        "C": 100,
                                        "XC": 90,
                                        "L": 50,
                                        "XL": 40,
                                        "X": 10,
                                        "IX": 9,
                                        "V": 5,
                                        "IV": 4,
                                        "I": 1
                                }
 
exports.run = (client, message, args) => {
  
  const db = require('quick.db');

  
        if(!args[0]) {
                const embed = new Discord.RichEmbed()
                        .setDescription('LÃ¼tfen sayÄ± yazÄ±n!')
                        .setColor("RANDOM")
                message.channel.send({embed})
                return
        }
 
        if(isNaN(args[0])) {
                const embed = new Discord.RichEmbed()
                        .setDescription('Sadece sayÄ± yazabilirsin')
                        .setColor("RANDOM")
                message.channel.send({embed})
                return
        }
       
        if(args[0] > 999999) {
                const embed = new Discord.RichEmbed()
                        .setDescription("LÃ¼tfen `999999` veya daha dÃ¼ÅŸÃ¼k bir sayÄ± yazÄ±n!!")
                        .setColor("RANDOM")
                message.channel.send({embed})
                return
        }
 
        const sayi = args[0]
 
        if(sayi === "0" || sayi === 0) {
                const embed = new Discord.RichEmbed()
                        .setDescription("KarÅŸÄ±lÄ±ksÄ±z")
                        .setColor("RANDOM")
                message.channel.send({embed})
                return
        }
 
        let result = '';
        for (const [numeral, value] of Object.entries(numerals)) {
                while (args[0] >= value) {
                        result += numeral;
                        args[0] -= value;
                }
        }
        const embed = new Discord.RichEmbed()
                .addField("YazÄ±lan SayÄ±", sayi)
                .addField("Romen KarÅŸÄ±lÄ±ÄŸÄ±", result)
                .setColor("RANDOM")
        message.channel.send({embed})
}
 
