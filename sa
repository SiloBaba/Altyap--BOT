const Discord = require('discord.js');
const hastebin = require('hastebin-gen');
exports.run = async (client, message, args, member) => {
  if (!args[0]) return message.channel.send("**Hata:** LÃ¼tfen **Bu Kanalda YazÄ±lmÄ±ÅŸ** bir mesajÄ±n id'sini girin!")
  if(args[0] < 18) return message.channel.send("**Hata:** LÃ¼tfen **Bu Kanalda YazÄ±lmÄ±ÅŸ** bir mesajÄ±n id'sini girin!")
  
   let number = ["1","2","3","4","5","6","7","8","9","0"]
    
    if (!number.some(word => message.content.includes(word))) {
           {
                  return message.channel.send("**Hata:** LÃ¼tfen **Bu Kanalda YazÄ±lmÄ±ÅŸ** bir mesajÄ±n id'sini girin!");
           }}   
  
  message.channel.fetchMessage(args[0]).then(dest => {
   hastebin(dest.content, "text").then(r => {
     var hastLink = r
let embed = new Discord.RichEmbed()
.setAuthor(`Bir Mesaj AlÄ±ntÄ±landÄ±!`)
.setDescription(`**Mesaj Sahibi:** ${dest.author.tag}\n**Mesaj Ä°Ã§eriÄŸi:** ${dest.content}\n**Mesaj Linki:** [TÄ±kla](https://discordapp.com/channels/${message.guild.id}/${message.channel.id}/${args[0]})`)
.setColor("RANDOM")
.setFooter(`${message.author.tag} tarafÄ±ndan istendi`, message.author.avatarURL)
message.channel.send(embed)
           message.react('617413726768988160')
}
)}).catch(err => {
    
 if (err.name === 'DiscordAPIError' && err.message === 'Unknown Message') {
       message.channel.send("**Hata:** LÃ¼tfen **Bu Kanalda YazÄ±lmÄ±ÅŸ** bir mesajÄ±n id'sini girin!"); 
   }
})}
