const Discord = require('discord.js');
const db = require('quick.db');

exports.run = async (client, message, args) => {

 

  let user = message.mentions.users.first();

  if (!user) return message.reply('Kime para yollayacaÄŸÄ±mÄ± etiketlemen lazÄ±m!');
  if (user.bot === true) return message.reply('ParayÄ± bir bota yollarsan sen zararlÄ± Ã§Ä±karsÄ±n!');
  
  let mesaj = args.slice(1).join(' ');
  if (!mesaj) return message.reply('YollayacaÄŸÄ±n miktarÄ± yazman gerek!');

  if (isNaN(args[1])) return message.channel.send("Bir yazÄ±mÄ± yollayacaksÄ±n? LÃ¼tfen bir miktar gir!.")
    

  let elmas = await db.fetch(`elmascÄ±klar_${user.id}`);
  let altÄ±n = await db.fetch(`altÄ±ncÄ±klar_${user.id}`);
  let para = await db.fetch(`paracÄ±k_${message.author.id}`);


  let prefix = await db.fetch(`prefix_${message.guild.id}`) || client.ayarlar.prefix;


const embed = new Discord.RichEmbed()
.setColor('RANDOM')
.setDescription(`${client.emojis.get(client.emojiler.paraGitti)} BaÅŸarÄ±yla ${user} adlÄ± kullanÄ±cÄ±ya para yollandÄ± \n YollanÄ±lan miktar: ${mesaj}`)
message.channel.send(embed)
db.add(`paracÄ±k_${user.id}`, mesaj)




