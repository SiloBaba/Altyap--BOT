exports.run = (client, message, args) => {
  
 //if(message.author.id !== "486817385051979786") return message.reply(`bu komutu sadece Bot Sahibi kullanabilir!`);

  let command;
  if (client.commands.has(args.slice(0).join(' '))) {
    command = args.slice(0).join(' ');
  } else if (client.aliases.has(args.slice(0).join(' '))) {
    command = client.aliases.get(args.slice(0).join(' '));
  }
  if (!args[0]) return message.reply("Bir komut ismi yazmalÄ±sÄ±n!")
  if (!command) {
	return message.reply("Botta `" + args.slice(0).join(' ') + "` isminde bir komut bulunmuyor!");
  } else {
    message.channel.send("`" + command + "` ismindeki komut devre dÄ±ÅŸÄ± bÄ±rakÄ±lÄ±yor...")
      .then(m => {
        client.unload(command)
          .then(() => {
            m.edit("`" + command + "` ismindeki komut baÅŸarÄ±yla devre dÄ±ÅŸÄ± bÄ±rakÄ±ldÄ±!");
          })
          .catch(e => {
            m.edit(`\`${command}\` isimli komut devre dÄ±ÅŸÄ± bÄ±rakÄ±lÄ±rken bir hata oluÅŸtu! \n**Hata:** \n\`\`\`js\n${e.stack}\n\`\`\``);
          });
      });
  }
    
};

